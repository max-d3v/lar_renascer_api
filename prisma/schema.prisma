generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model acolhidas {
  id                                Int       @id @default(autoincrement())
  nome                              String?   @db.Text
  data_nascimento                   DateTime? @db.Date
  rg                                String?   @db.VarChar(50)
  estado_civil                      String?   @db.VarChar(50)
  filiacao_genitor                  String?   @db.Text
  filiacao_genitora                 String?   @db.TinyText
  telefone                          String?   @db.VarChar(50)
  escolaridade                      String?   @db.VarChar(50)
  alfabetizada                      String?   @db.VarChar(50)
  experiencia_profissional          String?   @db.LongText
  funcao                            String?   @db.Text
  registro_carteira                 String?   @db.Text
  saude                             String?   @db.Text
  pre_natal                         String?   @db.VarChar(50)
  tempo_gestacao                    String?   @db.VarChar(50)
  apresenta_problema_saude          String?   @db.VarChar(50)
  problemas_saude                   String?   @db.LongText
  tratamento_problema_saude         String?   @db.LongText
  portador_necessidades_especiais   String?   @db.VarChar(50)
  necessidades_especiais            String?   @db.VarChar(50)
  tratamento_necessidades_especiais String?   @db.VarChar(50)
  cadastro_unico                    String?   @db.VarChar(50)
  bolsa_familia                     String?   @db.VarChar(50)
  valor_bolsa_familia               String?   @db.VarChar(50)
  pensao                            String?   @db.VarChar(50)
  valor_pensao                      String?   @db.VarChar(50)
  historia_pregressa_atual          String?   @db.LongText
  cpf                               String?   @db.VarChar(50)
  data_cadastro                     DateTime? @db.DateTime(0)

  @@index([cpf], map: "cpf")
}

model composicao_familiar {
  id              Int       @id @default(autoincrement())
  cpf_acolhida    Int?
  nome_familiar   String?   @db.Text
  parentesco      String?   @db.Text
  data_nascimento DateTime? @db.Date
  escolaridade    String?   @db.LongText
}

model sessions {
  session_id String  @id @db.VarChar(128)
  expires    Int     @db.UnsignedInt
  data       String? @db.MediumText
}

model usuarios {
  id      Int    @id @default(autoincrement())
  nome    String @default("0") @db.VarChar(250)
  tipo    String @default("0") @db.VarChar(250)
  usuario String @default("0") @db.VarChar(250)
  senha   String @default("0") @db.VarChar(250)
}
